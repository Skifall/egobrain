#include <stdio.h>
#include <stdlib.h>


int main()
{
srand( time( 0 ) );
int n=3;
int array[n];
int f = 1;
int i;
for(i = 0; i<n; i++)
    {
    array[i] = rand() % 3 -1;
    printf("%d\n", array[i]);
}
for(i = 0; i< (int)(n/2); i++)
    {
 if (array[i] !=array[n-i-1]) {
        f = 0;
        break;
    }
}
    if (f==1) {
    printf("\nTrue\n");
    }
    else {
    printf("\nFalse\n");
}
return 0;
}
