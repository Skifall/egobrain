#include <stdio.h>
#include <stdlib.h>

int main()
{
    int f=1;
    char string[255];
    free(string);
    fgets(string , 255, stdin);
    int i;
    for (i=0; i<strlen(string)-1; i++){
            if ((string[i]>= 'a')&&(string[i]<='z')) {
                string[i]=(char)((int)(string[i] - 32));
            }
            if ((string[i]>= 'а')&&(string[i]<='я')) {
                string[i]=(char)((int)(string[i] - 32));
            }
    }
    for(i = 0; i< (int)((strlen(string)-1)/2); i++)
    {
 if (string[i] !=string[strlen(string)-i-2]) {
        f = 0;
        break;
    }
}
    if (f==1) {
    printf("\nTrue\n");
    }
    else {
    printf("\nFalse\n");
}
return 0;
}
