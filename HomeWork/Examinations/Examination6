#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,j,k,f;
    scanf("%d", &k);
    for (i=2; i<=k; i++){
        f=1;
        for (j=2; j<=(int)(sqrt(i)); j++){
        if (i==2) {printf("2");}
        if ((i % j)==0) {f=0;}
        }
    if (f==1) {printf("%d\n", i);}
    }
    return 0;
}
